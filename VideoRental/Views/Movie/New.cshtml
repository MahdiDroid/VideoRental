@model  IEnumerable<VideoRental.Models.Genre>

<h2>New</h2>
<form>
    <div class="form-group">
        <label for="nameText">Name</label>
        <input type="text" class="form-control" id="nameText" aria-describedby="emailHelp">
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
        <label for="ReleaseDate">Release Date</label>
        <input type="text" class="form-control" id="ReleaseDate">
    </div>
    <div class="form-group">
        <label for="NumberInStock">Number InStock</label>
        <input type="text" class="form-control" id="NumberInStock">
    </div>

    <div class="form-group">
        <label for="GenreList">Genre</label>
        <select class="form-control" id="GenreList">
            @foreach (var g in @Model)
            {
                <option>@g.Name</option>
               
            }

        </select>
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
